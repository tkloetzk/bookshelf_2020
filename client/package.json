{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --verbose=false",
		"eject": "react-scripts eject",
		"test:watch": "react-scripts test --watch --verbose=false",
		"test:coverage": "react-scripts test --coverage --colors --watchAll=true"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}"
		],
		"coveragePathIgnorePatterns": [
			"<rootDir>/node_modules",
			"<rootDir>/src/setupTests.js",
			"<rootDir>/src/setupProxy.js",
			"<rootDir>/src/index.js",
			"<rootDir>/src/App/header/Header.js",
			"<rootDir>/src/config/",
			"<rootDir>/src/util/constants.js",
			"<rootDir>/src/store/configureStore.js",
			"<rootDir>/src/store/index.js"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@material-ui/core": "^4.9.5",
		"@material-ui/styles": "^4.9.0",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"react-redux": "^7.2.0",
		"react-swipeable-views": "^0.13.9",
		"redux": "^4.0.5"
	},
	"devDependencies": {
		"@stryker-mutator/core": "^2.5.0",
		"@stryker-mutator/html-reporter": "^2.5.0",
		"@stryker-mutator/javascript-mutator": "^2.5.0",
		"@stryker-mutator/jest-runner": "^2.5.0",
		"@testing-library/jest-dom": "^5.1.1",
		"@testing-library/react": "^9.4.1"
	}
}
